<?xml version="1.0" encoding="utf-8" ?> 
<font-family
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto">

  <!-- normal -->
  <font
    android:font="@font/urbanist_regular"
    android:fontStyle="normal"
    android:fontWeight="400"

    app:font="@font/urbanist_regular"
    app:fontStyle="normal"
    app:fontWeight="400" />

  <!-- Light -->
  <font
    android:font="@font/urbanist_light"
    android:fontStyle="normal"
    android:fontWeight="100"

    app:font="@font/urbanist_light"
    app:fontStyle="normal"
    app:fontWeight="100" />

  <!-- bold -->
  <font
    android:font="@font/urbanist_extrabold"
    android:fontStyle="normal"
    android:fontWeight="700"

    app:font="@font/urbanist_extrabold"
    app:fontStyle="normal"
    app:fontWeight="700" />

</font-family>